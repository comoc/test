	<FrameLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		
		<LinearLayout android:id="@+id/linearLayout1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">

			<LinearLayout android:id="@+id/linearLayout2"
				android:layout_width="match_parent" android:layout_height="wrap_content">

				<Button android:id="@+id/buttonStartFreeForm"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:drawableTop="@android:drawable/ic_menu_search"
					android:text="@string/startFreeForm" />

				<Button android:id="@+id/buttonStartWebSearch"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:drawableTop="@android:drawable/ic_menu_search"
					android:text="@string/startWebSearch" />

			</LinearLayout>

			<ListView android:id="@+id/listResults"
				android:layout_width="match_parent" android:layout_height="20mm"
				android:gravity="top|left" android:background="#000080" android:scrollbarAlwaysDrawVerticalTrack="true" android:scrollbars="vertical" android:scrollbarStyle="insideInset" android:fadeScrollbars="false" android:cacheColorHint="#00000000">
<!--
 				<requestFocus />
 -->
			</ListView>

			<LinearLayout android:id="@+id/linearLayout1"
				android:layout_width="match_parent" android:layout_height="wrap_content">
			</LinearLayout>
			<View android:layout_height="match_parent" android:layout_width="match_parent" android:background="@drawable/nashiji" android:id="@+id/viewTouch"></View>


		</LinearLayout>
	</FrameLayout>
</LinearLayout>

